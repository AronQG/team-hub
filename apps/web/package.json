{
  "name": "@team-hub/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate --schema=../../packages/db/prisma/schema.prisma",
    "db:push": "prisma db push --schema=../../packages/db/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=../../packages/db/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=../../packages/db/prisma/schema.prisma",
    "llm:test": "tsx examples/quickcheck.ts",
    "llm:test-all": "tsx examples/test-all-providers.ts",
    "llm:openai": "cross-env LLM_PROVIDER=openai tsx examples/quickcheck.ts",
    "llm:anthropic": "cross-env LLM_PROVIDER=anthropic tsx examples/quickcheck.ts",
    "llm:gemini": "cross-env LLM_PROVIDER=gemini tsx examples/quickcheck.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.50",
    "@ai-sdk/google": "^0.0.50",
    "@ai-sdk/openai": "^0.0.50",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "@hello-pangea/dnd": "^16.6.0",
    "@team-hub/db": "*",
    "ai": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "eventsource-parser": "^1.1.0",
    "jsonwebtoken": "^9.0.0",
    "next": "14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@google/generative-ai": "^0.24.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "8.57.1",
    "eslint-config-next": "15.4.6",
    "openai": "^4.104.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.20.4",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
